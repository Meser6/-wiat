{
    "printWidth": 140,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "printWidth": 160,
                "singleQuote": true
            }
        }
    ],
    "ignorePath": [".gitignore"],
    "plugins": ["prettier-plugin-organize-imports"]
}
